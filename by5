public class DivisibleByFive {
    public static void main(String[] args) {
        
        int num = Integer.parseInt(args[0]);

       
        if (num % 5 == 0) {
            System.out.println(num + " is divisible by 5");
        } else {
            System.out.println(num + " is not divisible by 5");
        }
    }
}
